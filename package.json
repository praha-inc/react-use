{
  "name": "@praha/react-kit",
  "version": "0.1.0",
  "description": "Collection of React Hooks and Utilities.",
  "keywords": [
    "javascript",
    "typescript",
    "react",
    "hooks"
  ],
  "homepage": "https://github.com/praha-inc/react-kit/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/praha-inc/react-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/praha-inc/react-kit.git"
  },
  "license": "MIT",
  "author": "PrAha, Inc.",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    },
    "import": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rslib build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint:code": "eslint .",
    "lint:type": "tsc --noEmit",
    "version": "changeset",
    "version:apply": "changeset version",
    "version:publish": "changeset publish",
    "version:prepublish": "pkg-pr-new publish --pnpm --compact"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.7",
    "@praha/eslint-config-common": "4.0.3",
    "@praha/eslint-config-definer": "2.0.2",
    "@praha/eslint-config-javascript": "3.0.9",
    "@praha/eslint-config-style": "3.0.10",
    "@praha/eslint-config-typescript": "3.0.10",
    "@praha/eslint-config-vitest": "1.0.5",
    "@rslib/core": "0.17.2",
    "@testing-library/react": "16.3.0",
    "@tsconfig/strictest": "2.0.8",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.1",
    "jsdom": "27.2.0",
    "pkg-pr-new": "0.0.60",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "typescript": "5.9.3",
    "vitest": "4.0.8"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "packageManager": "pnpm@10.20.0",
  "publishConfig": {
    "provenance": true
